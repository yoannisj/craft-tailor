{
  "name": "yoannisj/craft-tailor",
  "description": "Utilities to help with writing modular Twig templates in Craft CMS",
  "version": "1.0.0",
  "type": "craft-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Yoannis Jamar",
      "homepage": "https://yoannis.com"
    }
  ],
  "support": {
    "email": "office@yoannis.com",
    "issues": "https://github.com/yoannisj/craft-tailor/issues?state=open",
    "source": "https://github.com/yoannisj/craft-tailor",
    "docs": "https://github.com/yoannisj/craft-tailor/blob/master/README.md"
  },
  "require": {
    "craftcms/cms": "^3.1.0"
  },
  "autoload": {
    "psr-4": {
